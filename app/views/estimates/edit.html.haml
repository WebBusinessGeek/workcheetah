.container
  = simple_form_for @estimate, url: estimate_path(@estimate), method: :put do |f|
    %legend Edit the estimate below
    = render 'form', f: f
    = f.submit "Update Draft", id: "save_estimate", class: "btn btn-default"
    = f.submit "Send Estimate", id: "send_estimate", class: "btn btn-default"
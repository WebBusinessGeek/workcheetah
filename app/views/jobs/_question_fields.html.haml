.question.fields_for_set
  %fieldset
    .field-group
      .col-sm-8
        %label.inline Question Text
        = f.input :text, label: false, input_html: {class: "form-control"}, wrapper_html: {class: "input-append inline"}
      .col-sm-4.remover
        = f.hidden_field :_destroy
        = link_to "Remove Question", '#', class: "remove_fields btn-sm btn-primary", :style => "display: inline;"

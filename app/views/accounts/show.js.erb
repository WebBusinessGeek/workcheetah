var selector = $('#account')
<% if @part == :show %>
    selector.html('<%= j render("accounts/show") %>')
<% elsif @part == :company %>
    selector.html('<%= j render("accounts/customize") %>')
<% elsif @part == :payment_profile %>
    selector.html('<%= j render("payment_profiles/form") %>')
<% else @part == :login %>
    selector.html('<%= j render("users/edit") %>')
<% end %>